class Person {
    let name; var occupation

    constructor(name, occupation) {
        set this.name = name
        set this.occupation = occupation
    }

    mut setOccupation(newOccupation) {
        set this.occupation = newOccupation
    }
}


function main() {
    let me = Person("Chris", "programmer")

    set me.occupation = "developer"

    exampleFunctionCall()
}
