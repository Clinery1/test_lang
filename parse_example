interface Debug {
    fmt(mut formatter)
}

interface Default {
    function default()
}


class Person {
    let name; var occupation

    constructor(name, occupation) {
        set this.name = name
        set this.occupation = occupation
    }

    mut setOccupation(newOccupation) {
        set this.occupation = newOccupation
    }
}
implement Default for Person {
    function default() {
        Person("", "")
    }
}
implement Debug for Person {
    fmt(mut f) {
        f.writeStr("Person {name: ")
        f.writeStr(copy this.name)
        f.writeStr(", occupation: ")
        f.writeStr(copy this.occupation)
        f.writeStr("}")
    }
}


function main() {
    let me = Person("Chris", "programmer")

    set me.occupation = "developer"

    exampleFunctionCall()
}
